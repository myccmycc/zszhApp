<?xml version="1.0" encoding="utf-8"?>
<s:SkinnableContainer xmlns:fx="http://ns.adobe.com/mxml/2009" 
					  xmlns:s="library://ns.adobe.com/flex/spark" 
					  xmlns:mx="library://ns.adobe.com/flex/mx" width="775" height="510">
	<fx:Script>
		<![CDATA[
			import mx.managers.PopUpManager;
			
			
			[Embed(source="embeds/PopupWindow/B1.png")]
			[Bindable] 
			public var icon_B1:Class;
			
			
			private function StartApp():void {
				PopUpManager.removePopUp(this);
			}
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	 
	<fx:Style>
		
		.WelcomeTabs {
			
			tabWidth:165;
			tabHeight:33;
			fontFamily:微软雅黑;
			
			paddingTop:0;
			
			paddingLeft:0;
			
			horizontalGap:3;
			
			fontSize:16;
			
			text-color:#34495e;
			
			text-roll-over-color:#ffffff;
				
			fontWeight:normal;
			
			up-skin:ClassReference("zszh_welcome.Skin_WelcomeTab");
			
			down-skin:ClassReference("zszh_welcome.Skin_WelcomeTab");
			
			over-skin:ClassReference("zszh_welcome.Skin_WelcomeTab");
			
			disabled-skin:ClassReference("zszh_welcome.Skin_WelcomeTab");
			
			selected-up-skin:ClassReference("zszh_welcome.Skin_WelcomeTabSelected");
			
			selected-down-skin:ClassReference("zszh_welcome.Skin_WelcomeTabSelected");
			
			selected-over-skin:ClassReference("zszh_welcome.Skin_WelcomeTabSelected");
			
			selected-disabled-skin:ClassReference("zszh_welcome.Skin_WelcomeTabSelected");
			
			
		}
		
		.selectedTab {  
			font-size:11;  
			font-weight:normal;  
			color:#00ff00;  
		}  
	</fx:Style>
	<mx:TabNavigator width="100%" height="100%" tabStyleName="WelcomeTabs" backgroundColor="0x2980d7" >
		
		<s:NavigatorContent label="保存" width="100%" height="100%" backgroundColor="0xffffff" >
			<s:Button label="Save" skinClass="zszh_welcome.Skin_BtStart" width="178" height="62" horizontalCenter="-150" bottom="15" click="StartApp()" />
			<s:Button label="Cancel" skinClass="zszh_welcome.Skin_BtStart" width="178" height="62" horizontalCenter="150" bottom="15" click="StartApp()" />
			
			<s:SkinnableContainer backgroundColor="0xf2f0ec"  width="100%" height="290" bottom="100" >
				<s:Group>
					
					<s:Label />
					<s:Label />
					<s:TextInput />
					<s:TextInput />
					
					<s:ComboBox />
					<s:ComboBox />
					<s:ComboBox />
					<s:ComboBox />
					
				</s:Group>
			</s:SkinnableContainer>
		</s:NavigatorContent>
		
		
	</mx:TabNavigator>
	
</s:SkinnableContainer>
